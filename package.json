{
  "name": "habit-budget-tracker",
  "version": "1.0.0",
  "private": true,
  "description": "Habit & Budget Tracker - React, Express, Postgres, Tailwind",
  "proxy": "http://localhost:5000",
  "scripts": {
    "start": "cross-env PORT=3000 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "cross-env NODE_ENV=development PORT=5000 nodemon server/index.js",
    "server:prod": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "migrate": "node server/migrations/001-add-frequency-schedule-to-habits.js",
    "migrate:notes": "node server/migrations/002-create-daily-notes-table.js",
    "migrate:plans": "node server/migrations/003-create-budget-plans-table.js",
    "migrate:sleep": "node server/migrations/004-create-sleep-entries-table.js",
    "migrate:events": "node server/migrations/005-create-events-table.js",
    "migrate:calendar-notes": "node server/migrations/006-create-calendar-notes-table.js",
    "migrate:habit-notes": "node server/migrations/007-create-habit-notes-table.js",
    "migrate:daily-notes-title": "node server/migrations/008-add-title-to-daily-notes.js",
    "migrate:tasks": "node server/migrations/010-create-tasks-tables.js"
  },
  "dependencies": {
    "@twa-dev/sdk": "^8.0.2",
    "chart.js": "^4.4.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "react": "^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "recharts": "^3.6.0",
    "sequelize": "^6.37.3",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.14"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
